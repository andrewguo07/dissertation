@article{Fraxanet2022,
  title = {Topological {{Quantum Critical Points}} in the {{Extended Bose-Hubbard Model}}},
  author = {Fraxanet, Joana and {Gonz{\'a}lez-Cuadra}, Daniel and Pfau, Tilman and Lewenstein, Maciej and Langen, Tim and Barbiero, Luca},
  year = {2022},
  month = jan,
  journal = {Physical Review Letters},
  volume = {128},
  number = {4},
  pages = {043402},
  publisher = {{American Physical Society}},
  doi = {10.1103/PhysRevLett.128.043402},
  abstract = {The combination of topology and quantum criticality can give rise to an exotic mix of counterintuitive effects. Here, we show that unexpected topological properties take place in a paradigmatic strongly correlated Hamiltonian: the 1D extended Bose-Hubbard model. In particular, we reveal the presence of two distinct topological quantum critical points with localized edge states and gapless bulk excitations. Our results show that the topological critical points separate two phases, one topologically protected and the other topologically trivial, both characterized by a long-range ordered string correlation function. The long-range order persists also at the topological critical points and explains the presence of localized edge states protected by a finite charge gap. Finally, we introduce a superresolution quantum gas microscopy scheme for dipolar dysprosium atoms, which provides a reliable route towards the experimental study of topological quantum critical points.},
  file = {/Users/andrew/Zotero/storage/ND75HRC3/Fraxanet et al. - 2022 - Topological Quantum Critical Points in the Extende.pdf;/Users/andrew/Zotero/storage/CTHGFY8F/PhysRevLett.128.html}
}

