@article{Foss-Feig2016,
  title = {Entanglement and Spin-Squeezing without Infinite-Range Interactions},
  author = {{Foss-Feig}, Michael and Gong, Zhe-Xuan and Gorshkov, Alexey V. and Clark, Charles W.},
  year = {2016},
  eprint = {1612.07805},
  eprinttype = {arxiv},
  pages = {1--8},
  abstract = {Infinite-range interactions are known to facilitate the production of highly entangled states with applications in quantum information and metrology. However, many experimental systems have interactions that decay with distance, and the achievable benefits in this context are much less clear. Combining recent exact solutions with a controlled expansion in the system size, we analyze quench dynamics in Ising models with power-law (\$1/r\^\{\textbackslash alpha\}\$) interactions in \$D\$ dimensions, thereby expanding the understanding of spin squeezing into a broad and experimentally relevant context. In spatially homogeneous systems, we show that for small \$\textbackslash alpha\$ the scaling of squeezing with system size is identical to the infinite-range (\$\textbackslash alpha=0\$) case. This indifference to the interaction range persists up to a critical value \$\textbackslash alpha=2D/3\$, above which squeezing degrades continuously. Boundary-induced inhomogeneities present in most experimental systems modify this picture, but it nevertheless remains qualitatively correct for finite-sized systems.},
  archiveprefix = {arXiv}
}

